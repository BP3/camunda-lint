{
  "name": "camunda-linter-monorepo",
  "version": "0.0.2",
  "private": true,
  "workspaces": [
    "*mnlint-runner",
    "*mnlint-runner/bp3-dynamic-rules"
  ],
  "scripts": {
    "generate-sbom": "cyclonedx-bom",
    "test": "mocha ./test/**/*.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@BP3/bpmnlint-plugin-bpmn-rules": "npm:@BP3/bpmnlint-plugin-bpmn-rules",
    "bpmn-moddle": "^9.0.3",
    "bpmnlint": "^11.6.0",
    "bpmnlint-plugin-bp3global-bpmn-rules": "npm:@BP3/bpmnlint-plugin-bpmn-rules",
    "bpmnlint-utils": "^1.1.1",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "dmn-moddle": "^11.0.0",
    "dmnlint": "^0.2.0",
    "dmnlint-utils": "^0.1.0",
    "junit-report-builder": "^5.1.1",
    "semver": "^7.7.2",
    "tiny-glob": "^0.2.9",
    "yargs": "^18.0.0",
    "zeebe-bpmn-moddle": "^1.11.0"
  },
  "devDependencies": {
    "chai": "^6.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "mocha": "^11.7.5",
    "prettier": "^3.6.2"
  },
  "overrides": {
    "@BP3/bpmnlint-plugin-bpmn-rules": "$@BP3/bpmnlint-plugin-bpmn-rules",
    "bpmn-moddle": "$bpmn-moddle",
    "bpmnlint": "$bpmnlint",
    "bpmnlint-plugin-camunda-compat": "$bpmnlint-plugin-camunda-compat",
    "bpmnlint-utils": "$bpmnlint-utils",
    "dmn-moddle": "$dmn-moddle",
    "dmnlint": "$dmnlint",
    "dmnlint-utils": "$dmnlint-utils"
  }
}
