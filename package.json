{
  "name": "camunda-linter-monorepo",
  "version": "0.0.2",
  "private": true,
  "workspaces": [
    "*mnlint-runner",
    "*mnlint-runner/bp3-dynamic-rules"
  ],
  "scripts": {
    "test": "mocha ./test/**/*.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "bpmn-moddle": "^9.0.3",
    "bpmnlint": "^11.6.0",
    "bpmnlint-utils": "^1.1.1",
    "chalk": "^4.1.2",
    "dmn-moddle": "^11.0.0",
    "dmnlint": "^0.2.0",
    "dmnlint-utils": "^0.1.0",
    "junit-report-builder": "^5.1.1",
    "tiny-glob": "^0.2.9",
    "yargs": "^18.0.0",
    "zeebe-bpmn-moddle": "^1.12.0"
  },
  "devDependencies": {
    "chai": "^6.2.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "mocha": "^11.7.5",
    "prettier": "^3.6.2"
  },
  "overrides": {
    "bpmn-moddle": "$bpmn-moddle",
    "bpmnlint": "$bpmnlint",
    "bpmnlint-utils": "$bpmnlint-utils",
    "dmn-moddle": "$dmn-moddle",
    "dmnlint": "$dmnlint",
    "dmnlint-utils": "$dmnlint-utils",
    "zeebe-bpmn-moddle": "$zeebe-bpmn-moddle",
    "chalk": "$chalk",
    "cli-table3": "$cli-table3",
    "junit-report-builder": "$junit-report-builder",
    "tiny-glob": "$tiny-glob",
    "yargs": "$yargs",
    "eslint": "$eslint",
    "eslint-config-prettier": "$eslint-config-prettier",
    "eslint-plugin-prettier": "$eslint-plugin-prettier",
    "globals": "$globals",
    "prettier": "$prettier"
  }
}
